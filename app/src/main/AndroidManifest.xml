<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.institutemanagement">
    <!--    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Register" />
        <activity android:name=".DeleteStudents" />
        <activity android:name=".UpdateStudents" />
        <activity android:name=".AddNewStudents" />
        <activity android:name=".DeleteFaculty" />
        <activity android:name=".UpdateFaculty" />
        <activity android:name=".AddNewFaculty" />
        <activity android:name=".DeleteCourses" />
        <activity android:name=".UpdateCourses" />
        <activity android:name=".AddNewCourses" />
        <activity android:name=".StudentsMainMenu" />
        <activity android:name=".FacultyMainMenu" />
        <activity android:name=".CoursesMainMenu" />
        <activity android:name=".MainMenu" />
        <activity android:name=".Login" />
        <activity android:name=".Introduction" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>